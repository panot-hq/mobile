{
  "expo": {
    "name": "Panot",
    "slug": "Panot",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "Panot",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "icon": {
        "dark": "./assets/icons/ios-dark.png",
        "light": "./assets/icons/ios-light.png",
        "tinted": "./assets/icons/ios-tinted.png"
      },
      "supportsTablet": true,
      "bundleIdentifier": "com.ahrm.panot",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSSpeechRecognitionUsageDescription": "Allow $(PRODUCT_NAME) to use speech recognition.",
        "NSMicrophoneUsageDescription": "Allow $(PRODUCT_NAME) to use the microphone.",
        "NSContactsUsageDescription": "Allow $(PRODUCT_NAME) to access your contacts."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.ahrm.panot"
    },
    "web": {
      "bundler": "metro",
      "output": "server"
    },
    "plugins": [
      "expo-localization",
      [
        "expo-router",
        {
          "origin": "https://panot.com"
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/icons/splash-icon-dark.png",
          "resizeMode": "contain",
          "backgroundColor": "#000",
          "dark": {
            "image": "./assets/icons/splash-icon-light.png",
            "resizeMode": "contain",
            "backgroundColor": "#fff"
          }
        }
      ],
      [
        "expo-contacts",
        {
          "contactsPermission": "Allow $(PRODUCT_NAME) to access your contacts."
        }
      ],
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "merchant.com.ahrm.panot",
          "publishableKey": "pk_test_51SVcCbJXKGjQr9YZXn0gkfU7PXKmZ6MNW7cA5cvgSWGPH18X8W70QbCPC5xzi3neTLQaPckSahP0E4Cv45BDBpYp00TYe4lfqN"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "72501028-5edd-466b-b1c0-7708941aaea3"
      }
    }
  }
}
